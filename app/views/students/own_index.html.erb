<p id="notice"><%= notice %></p>

<h1>My Students</h1>

<table class="basic_table">
  <thead>
  <tr>
    <th>Name</th>
    <th>Last Name</th>
    <th>Student RUT </th>
    <th>Assistance</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @students.each do |student| %>
      <tr>
        <td><%= student.name %></td>
        <td><%= student.last_name %></td>
        <td><%= student.rut %></td>
        <td ><%= link_to 'See Assistance', student, class:'blue_btn' %></td>
        <td ><%= link_to 'Generate QR', generate_qr_student_path(student.id), class:'blue_btn' %><td >
      </tr>
  <% end %>
  </tbody>
</table>

<br>
