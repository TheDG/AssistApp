<p id="notice"><%= notice %></p>

<h1>Assistances</h1>

<table class="w3-table-all w3-centered">
  <thead>
    <tr>
      <th>Student</th>
      <th>Date</th>
      <th>Course ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @assistance.each do |assistance| %>
      <tr>
        <td><%= assistance.student.rut %></td>
        <td><%= assistance.date.strftime('%d-%m-%Y') %></td>
        <td><%= assistance.course_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

