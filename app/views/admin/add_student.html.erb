
<p id="notice"><%= notice %></p>

<h1>Course</h1>

<table>
  <thead>
  <tr>
    <th>Teacher</th>
    <th>Subject</th>
    <th>Grade</th>
    <th>Level</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @courses.each do |course| %>
      <tr>
        <td><%= course.teacher.name %></td>
        <td><%= course.subject %></td>
        <td><%= course.grade %></td>
        <td><%= course.level %></td>
        <td><%= link_to 'Add student', admin_add_student2_path(course: course.id) %></td>
      </tr>
  <% end %>
  </tbody>
</table>


<br>

<%= link_to 'New Course', new_course_path %>

