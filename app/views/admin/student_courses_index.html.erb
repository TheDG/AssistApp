<h1>Students Enroled </h1>

<table class="basic_table">
  <thead>
  <tr>
    <th>Subject1</th>
    <th>Grade</th>
    <th>Level</th>
    <th>Student RUT </th>
    <th>Assistance</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @courses.each do |course| %>
        <% course.students.each do |students| %>
          <tr>
            <td><%= course.subject %></td>
            <td><%= course.grade %></td>
            <td><%= course.level %></td>
            <td><%= students.rut %></td>
            <td ><%= link_to 'See Assistance', students, class:'blue_btn' %></td>
          </tr>
  <% end %>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course', new_course_path, class:'blue_btn' %>
